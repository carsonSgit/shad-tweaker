{
  "name": "shadcn-tweaker",
  "version": "1.0.0",
  "description": "Terminal-based tool for batch customizing shadcn/ui components",
  "type": "module",
  "bin": {
    "shadcn-tweaker": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "npm run build:cli && npm run build:backend && npm run build:frontend",
    "build:cli": "tsc -p cli/tsconfig.json",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "dev": "tsx cli/index.ts",
    "start": "node dist/cli/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "shadcn",
    "ui",
    "cli",
    "tui",
    "components",
    "tailwind",
    "customization"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "get-port": "^7.0.0",
    "cors": "^2.8.5",
    "diff": "^7.0.0",
    "express": "^4.18.2",
    "uuid": "^9.0.1",
    "ink": "^5.2.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "cli-highlight": "^2.1.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/diff": "^7.0.0",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@types/react": "^18.3.17",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "backend/dist",
    "backend/package.json",
    "frontend/dist",
    "frontend/package.json",
    "docs",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": ""
  }
}
